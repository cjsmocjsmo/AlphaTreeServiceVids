---
    const {src} = Astro.props;
---

<div class="vidWrapper">
	<iframe 
		height="334"
		width="593"
		class="iframeWrapper"
		src={src} 
		title="Texas Trip video player" 
		frameborder="0" 
		allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" 
		allowfullscreen>
	</iframe>
</div> 